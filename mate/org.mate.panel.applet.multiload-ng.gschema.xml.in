<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema id="org.mate.panel.applet.multiload-ng">
    <key name="interval" type="i">
      <default>500</default>
    </key>
    <key name="size" type="i">
      <default>40</default>
    </key>
    <key name="padding" type="i">
      <default>2</default>
    </key>
    <key name="spacing" type="i">
      <default>1</default>
    </key>
    <key name="orientation" type="i">
      <default>0</default>
    </key>
    <key name="fill-between" type="b">
      <default>false</default>
    </key>
    <key name="tooltip-details" type="b">
      <default>false</default>
    </key>
    <key name="dblclick-policy" type="i">
      <default>0</default>
    </key>
    <key name="dblclick-cmdline" type="s">
      <default>''</default>
    </key>

    <key name="graph-cpuload-visible" type="b">
      <default>true</default>
    </key>
    <key name="graph-cpuload-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-cpuload-colors" type="s">
      <default>''</default>
    </key>

    <key name="graph-memload-visible" type="b">
      <default>false</default>
    </key>
    <key name="graph-memload-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-memload-colors" type="s">
      <default>''</default>
    </key>

    <key name="graph-netload-visible" type="b">
      <default>false</default>
    </key>
    <key name="graph-netload-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-netload-colors" type="s">
      <default>''</default>
    </key>

    <key name="graph-swapload-visible" type="b">
      <default>false</default>
    </key>
    <key name="graph-swapload-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-swapload-colors" type="s">
      <default>''</default>
    </key>

    <key name="graph-loadavg-visible" type="b">
      <default>false</default>
    </key>
    <key name="graph-loadavg-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-loadavg-colors" type="s">
      <default>''</default>
    </key>

    <key name="graph-diskload-visible" type="b">
      <default>false</default>
    </key>
    <key name="graph-diskload-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-diskload-colors" type="s">
      <default>''</default>
    </key>

    <key name="graph-temperature-visible" type="b">
      <default>false</default>
    </key>
    <key name="graph-temperature-border-width" type="i">
      <default>1</default>
    </key>
    <key name="graph-temperature-colors" type="s">
      <default>''</default>
    </key>
  </schema>
</schemalist>